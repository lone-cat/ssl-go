{
  "env": "dev",
  "email": "",
  "domains": [],
  "port": 8080,
  "keyLength": 4096,
  "certDaysLeftMin": 30,
  "useStaging": true,
  "storage": {
    "root": "certs",
    "accountKey": "account.key",
    "certificateKey": "certificate.key",
    "certificates": {
      "fullChain": "fullchain.pem",
      "split": {
        "certificate": "certificate.pem",
        "intermediates": "intermediate{n}.pem"
      }
    }
  },
  "formats": [
    {
      "filename": "abc/filename.pem"
    },
    {
      "folder": "/def",
      "filename": "abc/filename.pem"
    },
    {
      "folder": "h",
      "privateKeyFilename": "certificate.key",
      "certificateFilename": "certificate.pem",
      "intermediatePattern": "./intermediate{n}.pem"
    },
    {
      "privateKeyFilename": "certificate.key",
      "certificateFilename": "certificate.pem",
      "intermediatePattern": "abc/intermediate{n}.pem"
    }
  ]
}